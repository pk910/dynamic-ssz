    "version": "0.2.0",
    "configurations": [

      
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "cwd": "${workspaceFolder}/test",
            "program": "${workspaceFolder}/test",
            "args": [
              "performance"
            ]
        },
        {
            "name": "Launch test function",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}",
            "args": [
              "-test.run",
              // test function name
              // * can use reguler expression
              // * NOT include "Test"
              // * the first charactor MUST be small
              ""
            ]
          },
        {
            "name": "Launch codegen test function",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/codegen",
            "args": [
              "-test.run",
              // test function name
              // * can use reguler expression
              // * NOT include "Test"
              // * the first charactor MUST be small
              ""
            ]
          },
          {
            "name": "Launch spectests",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/spectests",
            "env": {
                "CONSENSUS_SPEC_TESTS_DIR": "${workspaceFolder}/spectests/consensus-spec-tests/tests"
            },
            "args": [
              "-test.run",
              ""
            ]
        },
          {
            "name": "Fuzz: MarshalUnmarshal",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/fuzz",
            "env": {
            },
            "args": [
              "-test.fuzz=FuzzMarshalUnmarshal",
              "-test.fuzztime=30s",
              "-test.fuzzcachedir=${workspaceFolder}/fuzz/testdata/fuzz"
            ]
        },
        {
            "name": "Fuzz: Size",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/fuzz",
            "env": {
            },
            "args": [
              "-test.fuzz=FuzzSize",
              "-test.fuzztime=30s",
              "-test.fuzzcachedir=${workspaceFolder}/fuzz/testdata/fuzz"
            ]
        },
        {
            "name": "Fuzz: HashTreeRoot",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/fuzz",
            "env": {
            },
            "args": [
              "-test.fuzz=FuzzHashTreeRoot",
              "-test.fuzztime=30s",
              "-test.fuzzcachedir=${workspaceFolder}/fuzz/testdata/fuzz"
            ]
        },
        {
            "name": "Test: Fuzz Basic",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/fuzz",
            "env": {
            },
            "args": [
              "-test.run=TestFuzzerBasic"
            ]
        },

        {
            "name": "Launch pyssz test_multidim_slice.go",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/test/pyssz/test_multidim_slice.go"
        },
        {
            "name": "Launch pyssz test_progressive_container.go",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/test/pyssz/test_progressive_container.go"
        },
        {
          "name": "Launch codegen",
          "type": "go",
          "request": "launch",
          "mode": "auto",
          "cwd": "${workspaceFolder}/test",
          "program": "${workspaceFolder}/test",
          "args": [
            "codegen"
          ]
      },
      {
        "name": "Launch codegenTest",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "cwd": "${workspaceFolder}/test",
        "program": "${workspaceFolder}/test",
        "args": [
          "codegen-test"
        ]
    }
    ]
}